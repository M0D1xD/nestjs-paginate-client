{
  "name": "nestjs-paginate-client",
  "version": "0.0.1",
  "description": "Type-safe query string builder for nestjs-paginate compatible backends. This package is a client-side library for the nestjs-paginate package.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M0D1xD/nestjs-paginate-client.git"
  },
  "license": "MIT",
  "author": {
    "name": "Mohammed Zakout",
    "email": "info@zakout.tech"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "commit": "cz",
    "prepare": "husky",
    "format": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "lint": "eslint ./src --max-warnings 0",
    "tsc:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@release-it/conventional-changelog": "10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.16.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "release-it": "^19.2.4",
    "semantic-release": "^25.0.3",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18"
  },
  "keywords": [
    "@nestjs",
    "nestjs-paginate",
    "pagination",
    "query-string",
    "typescript"
  ],
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "node": ">=20.19.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
